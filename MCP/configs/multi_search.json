{
  "name": "multi-search-suite",
  "description": "Suite complète de recherche multi-providers : DuckDuckGo, Brave, Google, SerpAPI, Tavily",
  "version": "1.0.0",
  "providers": {
    "duckduckgo": {
      "name": "DuckDuckGo Search",
      "description": "Recherche privée sans clé API requise", 
      "api_key_required": false,
      "privacy": "high",
      "rate_limit": "no_limit",
      "cost": "free"
    },
    "brave": {
      "name": "Brave Search", 
      "description": "Recherche sans tracking avec clés API",
      "api_key_required": true,
      "privacy": "high", 
      "rate_limit": "2000_per_month",
      "cost": "free_tier"
    },
    "google_custom": {
      "name": "Google Custom Search",
      "description": "Recherche Google avec API officielle",
      "api_key_required": true,
      "privacy": "medium",
      "rate_limit": "100_per_day", 
      "cost": "free_tier"
    },
    "serpapi": {
      "name": "SerpAPI",
      "description": "Google search avec proxy et captcha handling",
      "api_key_required": true,
      "privacy": "medium",
      "rate_limit": "plan_based",
      "cost": "paid"
    },
    "tavily": {
      "name": "Tavily Search",
      "description": "Recherche optimisée pour AI avec citations",
      "api_key_required": true, 
      "privacy": "medium",
      "rate_limit": "1000_per_month",
      "cost": "free_tier"
    }
  },
  "capabilities": [
    "web_search",
    "news_search", 
    "image_search",
    "video_search",
    "academic_search",
    "real_time_search",
    "fallback_search", 
    "multi_provider_search",
    "privacy_focused_search"
  ],
  "fallback_order": [
    "duckduckgo",
    "brave", 
    "tavily",
    "google_custom",
    "serpapi"
  ],
  "security": {
    "api_key_rotation": true,
    "rate_limiting": true,
    "privacy_protection": true,
    "no_tracking": ["duckduckgo", "brave"]
  }
}